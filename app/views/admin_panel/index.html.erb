<div class="container">
<h1>Admin Panel</h1>
<table class="table">
	<tr>
	<th>ID</th>
	<th>First Name</th>
	<th>Last Name</th>
	<th>Email</th>
	<th>Nick</th>
	<th>User Role</th>
	<th>Action</th>
	</tr>
	<% @user.each do |u| %>
	<tr>	
		<td><%= u.id %>
		<td><%= u.first_name %></td>
		<td><%=	u.last_name	%></td>
		<td><%=	u.email	%></td>
		<td><%=	u.nick	%></td>
		<td><%=	u.user_role	%></td>
	</tr>
	</br>
	<%end%>
</table>
</div>